= render :partial => 'layouts/admin_nav'
%header.page-header
  %h1.settings=t 'settings.settings'

%section.content-section.settings
  %header
    %h2= t('.permissions')
  .content
    - if check_privilege(Privilege::MODIFY)
      %p
        = t('.permissions_desc')
        = link_to t(:continue), self_service_settings_path, :class => 'button'
